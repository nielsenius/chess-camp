<% if @registrations.any? %>
  <!-- <h3>&nbsp;</h3> -->
  <div class="panel clearfix">
    <h5>Registrations</h5>
    <ul class="side-nav">
      <% @registrations.each do |registration| %>
        <li><%= link_to registration.name, student_path(registration) %></li>
      <% end %>
    </ul>
  </div>

<% else %>
  <div class="panel clearfix">
    <h5>No Registrations</h5>
  </div>
<% end %>
